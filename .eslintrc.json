{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
    "consistent-return": 0,
    "func-names": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-prototype-builtins": 0,
    "no-restricted-globals": 0,
    "no-restricted-syntax": 0,
    "no-return-assign": 0,
    "no-undef": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0         
  }
}
